<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.skku_food">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/icon"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".foodcupActivity.ResFinishActivity"
                  android:screenOrientation="portrait">
        </activity>
        <activity android:name=".foodcupActivity.ResTournamentActivity"
                  android:screenOrientation="portrait">
        </activity>
        <activity
                android:name=".foodcupActivity.MenuFinishActivity"
                android:screenOrientation="portrait">
        </activity>
        <activity
                android:name=".foodcupActivity.MenuTournamentActivity"
                android:screenOrientation="portrait">
        </activity>
        <activity
                android:name=".menuActivity.MapActivity"
                android:screenOrientation="portrait">
        </activity>
        <activity
                android:name=".menuActivity.ResMenuActivity"
                android:screenOrientation="portrait">
        </activity>
        <activity
                android:name=".menuActivity.RestaurantActivity"
                android:screenOrientation="portrait">
        </activity>
        <activity
                android:name=".SplashActivity"
                android:screenOrientation="portrait"
                android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <action android:name="android.intent.action.VIEW"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
                android:name=".MainActivity"
                android:screenOrientation="portrait"/>

        <meta-data
                android:name="com.google.android.gms.ads.APPLICATION_ID"
                android:value="@string/admob_app_id"/>
        <meta-data
                android:name="com.kakao.sdk.AppKey"
                android:value="key"/>
    </application>

</manifest>
